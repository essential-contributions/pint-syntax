{
  "name": "pint-lang",
  "displayName": "Pint Syntax",
  "description": "Syntax highlighting for pint lang",
  "version": "0.0.2",
  "publisher": "essential-contributions",
  "repository": {
    "type": "git",
    "url": "https://github.com/essential-contributions/pint-syntax"
  },
  "license": "Apache-2.0",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
        "pint",
        "language",
        "syntax",
        "grammar",
        "highlighting",
        "constraints"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pint",
        "aliases": [
          "Pint",
          "pint"
        ],
        "extensions": [
          "pnt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pint",
        "scopeName": "source.pint",
        "path": "./syntaxes/pint.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "js-yaml": "^4.1.0",
    "@vscode/vsce": "^2.27.0"
  }
}
